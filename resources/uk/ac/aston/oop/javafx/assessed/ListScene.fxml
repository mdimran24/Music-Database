<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<VBox spacing="5.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ListView fx:id="listItems" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
      <HBox alignment="CENTER" spacing="5.0">
         <children>
            <Button mnemonicParsing="false" onAction="#shufflePressed" text="Shuffle" />
            <Button mnemonicParsing="false" onAction="#addPressed" text="Add" />
            <Button mnemonicParsing="false" onAction="#removePressed" text="Remove" />
            <Button mnemonicParsing="false" onAction="#quitPressed" text="Quit" />
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </HBox>
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</VBox>
